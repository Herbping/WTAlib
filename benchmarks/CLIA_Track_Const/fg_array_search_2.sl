(set-logic LIA)
(synth-fun findIdx ( (y1 Int) (y2 Int) (kt1 Int)) Int )
(declare-var x1 Int)
(declare-var x2 Int)
(declare-var kt Int)
(constraint (=> (< x1 x2) (=> (< kt x1) (= (findIdx x1 x2 kt) 0))))
(constraint (=> (< x1 x2) (=> (> kt x2) (= (findIdx x1 x2 kt) 2))))
(constraint (=> (< x1 x2) (=> (and (> kt x1) (< kt x2)) (= (findIdx x1 x2 kt) 1))))
(check-synth)
