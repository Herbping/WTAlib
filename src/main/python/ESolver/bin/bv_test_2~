(set-logic BV)
(define-fun e ((x (BitVec 8))) (BitVec 8)  (bvadd #x04 (bvsub #x02 (bvadd x #x04))) )

(synth-fun d ((x (BitVec 8))) (BitVec 8)
    ((Start (BitVec 8) (
						 (bvsub Start Start)
                            x #x01 #x02 #x03 #x04 #x05)) 
))


(declare-var x (BitVec 8))
(declare-var y (BitVec 8))
(constraint 
			(= (d (e x)) x)
)

(check-synth)    


